# Feature Flags - Control which advanced features are enabled
# Set to 'true' to enable, 'false' or leave empty to disable

# Advanced Profile Features
REACT_APP_ENABLE_ADVANCED_PROFILE=false

# AI Insights and Complex Models
REACT_APP_ENABLE_AI_INSIGHTS=false
REACT_APP_ENABLE_COMPLEX_MODELS=false

# Enhanced Onboarding
REACT_APP_ENABLE_ENHANCED_ONBOARDING=false

# Experimental Features
REACT_APP_ENABLE_EXPERIMENTAL=false

# AI Provider Configuration
# Gemini Pro (Primary) - Get your API key from: https://makersuite.google.com/app/apikey
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Alternative providers (not currently used)
# REACT_APP_CLAUDE_API_KEY=your_claude_api_key_here
# REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Environment
NODE_ENV=development
